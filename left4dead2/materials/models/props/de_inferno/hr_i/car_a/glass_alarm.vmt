VertexLitGeneric
{
"$baseTexture" "models\props\de_inferno\hr_i\car_a\glass"

	//$basealphaenvmapmask 1
	"$envmap" "env_cubemap"
	"$envmaptint" 		"[.2 .2 .2]"
	
	"$phongexponent" 12
	"$phongexponenttexture" "dev\reflectivity_10b"
	"$phongalbedotint" "1"
	//"$phongtint" "[.1 .15 .1]"
	"$phongdisablehalflambert" "1"
	"$phong" "1"
	"$phongboost" "6"
	"$phongfresnelranges" "[.8 0.5 1]"	 
	"$basemapalphaphongmask"	"1"
	
$selfillum 1
$selfillummask "vgui/white"
$bumpmap "dev\flat_normal"
//$translucent 1
$halflambert 1

$model 1

//$invertphongmask 1
$phongfresnelranges "[.3 .6 7]"





$redOscillation 1.0
$greenOscillation 1.0
$blueOscillation 1.0
Proxies
{
Sine
{
resultVar $redOscillation
sineperiod .8
sinemin -2.7776 
sinemax 0.6944
}
Sine
{
resultVar $greenOscillation
sineperiod .8
sinemin -0.8
sinemax 0.2
}
Sine
{
resultVar $blueOscillation
sineperiod .8
sinemin -0.48
sinemax 0.12
}
Clamp
{
srcVar1 $redOscillation
min 0
max 1
resultVar "$selfillumtint[0]"
}
Clamp
{
srcVar1 $greenOscillation
min 0
max 1
resultVar "$selfillumtint[1]"
}
Clamp
{
srcVar1 $blueOscillation
min 0
max 1
resultVar "$selfillumtint[2]"
}
}
}